{
  "name": "codingbyjerez/minecraft-bundle",
  "type": "symfony-bundle",
  "description": "Bundle for info server Minecraft",
  "keywords": [
    "Minecraft"
  ],
  "homepage": "http://coding.by.jerez.me",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodolphe Jerez",
      "email": "rodolphe@jerez.me"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "symfony/framework-bundle": "~4.2",
    "symfony/translation": "~4.2"
  },

  "require-dev": {},
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "CodingByJerez\\MinecraftBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CodingByJerez\\MinecraftBundle\\Tests\\": "/Tests/"
    }
  }
}